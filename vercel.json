{
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        }
      ]
    },
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable, s-maxage=604800"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/schema",
      "destination": "/index.html"
    },
    {
      "source": "/schedule",
      "destination": "/index.html"
    },
    {
      "source": "/single-game",
      "destination": "/index.html"
    },
    {
      "source": "/match/(.*)",
      "destination": "/index.html"
    }
  ]
}
